{
  "tasks": {
    "start": "deno run --allow-env --allow-net --allow-read --allow-ffi --unstable main.ts",
    "fmt": "deno fmt",
    "lint": "deno lint"
  },
  "fmt": {
    "options": {
      "indentWidth": 2,
      "lineWidth": 100,
      "useTabs": false
    }
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "links": [
    "../others/deno_python"
  ],
  "nodeModulesDir": "auto"
}
